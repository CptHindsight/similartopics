{
    "name": "vse/similartopics",
    "type": "phpbb-extension",
    "description": "Displays a list of similar (related) topics at the bottom of the current topic's page.",
    "homepage": "https://github.com/VSEphpbb/similartopics",
    "version": "1.3.0-b2",
    "keywords": ["phpbb", "extension", "similar", "topics"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Matt Friedman (VSE)",
            "homepage": "http://mattfriedman.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Precise Similar Topics",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0-RC2,<3.2.*@dev"
        },
        "version-check": {
            "host": "vsephpbb.github.io",
            "directory": "/similartopics",
            "filename": "version.json"
        }
    }
}
